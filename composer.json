{
    "name": "kmchan/sculpin-execute-bundle",
    "description": "Provides a Twig execute filter for executing commands and embedding its result",
    "version": "0.0.1",
    "license": "MIT",

    "authors": [
        {
            "name": "Chan Kwun Man"
        }
    ],

    "require": {
        "php": "^7.2",
        "sculpin/sculpin": "3.0.*",
        "symfony/process": "^4.1"
    },

    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "^8",
        "phpstan/phpstan": "^0.12"
    },

    "autoload": {
        "psr-4": {
            "Kmchan\\Sculpin\\ExecuteBundle\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Kmchan\\Sculpin\\ExecuteBundle\\Tests\\": "tests/"
        }
    }
}
